---
interface Props
{
    title: string;
    higlight: string;
    alignText: string;
    align: string;
    color?: string;
}

const props = Astro.props
---

<div class:list={[props.alignText, props.align]}>
    <div class="_14">
        {props.title} <span style={`color: ${props.color}`}>{props.higlight}</span>
    </div>
</div>